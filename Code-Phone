import socket

# Create a new TCP socket using IPv4
client = socket.socket(socket.AF_INET, socket.SOCK_STREAM)

# Connect to the server's IP address and listening port
client.connect(("192.168.0.100", 12345))

# Send a message to the server (will be printed on the laptop)
client.sendall(b"Hello, Laptop!")

# Close the socket after sending the message
client.close()
